{
	"name": "mobile-open-platform",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"ant-design-pro": "^2.1.1",
		"antd": "^3.11.2",
		"mango-motion-web": "0.0.4",
		"mango-web": "^0.0.2",
		"rc-texty": "^0.2.0",
		"react": "^16.7.0",
		"react-dom": "^16.6.3"
	},
	"scripts": {
		"start": "node scripts/start.js",
		"build": "node scripts/build.js",
		"build:dev": "node scripts/build.js",
		"test": "node scripts/test.js",
		"lint": "./node_modules/.bin/eslint js",
		"precommit": "lint-staged"
	},
	"lint-staged": {
		"js/**/*.js": [
			"eslint",
			"git add"
		]
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"babel": {
		"presets": [
			"react-app"
		],
		"plugins": [
			[
				"@babel/plugin-proposal-decorators",
				{
					"legacy": true
				}
			]
		]
	},
	"devDependencies": {
		"less": "^2.7.3",
		"less-loader": "^4.1.0"
	}
}
